{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "dataramen",
  "version": "0.1.0",
  "identifier": "app.dataramen.xyz",
  "build": {
    "frontendDist": "../dist/web",
    "beforeBuildCommand": "",
    "beforeDevCommand": ""
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "dataramen",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "externalBin": ["binaries/my-sidecar"]
  }
}
